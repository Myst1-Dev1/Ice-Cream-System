generator client {
  provider     = "prisma-client"
  output       = "../generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model Sale {
  id        Int      @id @default(autoincrement())
  category  String
  flavor    String
  price     Float
  amount    Int?
  type      SaleType
  createdAt DateTime @default(now())
}

enum SaleType {
  venda
  entrada
}
